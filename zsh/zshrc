# Load necessary files
if [ -d $HOME/.zsh ]; then
  for file in $HOME/.zsh/*.zsh; do
    source $file
  done
fi

# Support 256-color terminals
export TERM=screen-256color

export WK=~/workspace/

# Ruby rbenv
export PATH="$HOME/.rbenv/bin:$PATH"

# Go
export GOPATH="$HOME/workspace/go"
export PATH=$PATH:/usr/local/go/bin
